{
  "name": "scottfasone-events",
  "version": "1.1.0",
  "description": "A web application for managing and sharing events",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "yarn run test:int && yarn run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" yarn exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@libsql/client": "^0.15.14",
    "@payloadcms/db-sqlite": "3.53.0",
    "@payloadcms/next": "3.53.0",
    "@payloadcms/richtext-lexical": "3.53.0",
    "@payloadcms/ui": "3.53.0",
    "@tailwindcss/postcss": "^4.1.12",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.14",
    "dotenv": "16.4.7",
    "graphql": "^16.8.1",
    "leaflet": "^1.9.4",
    "leaflet-defaulticon-compatibility": "^0.1.2",
    "next": "15.4.4",
    "payload": "3.53.0",
    "postcss": "^8.5.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-leaflet": "^5.0.0",
    "sharp": "0.34.2",
    "tailwindcss": "^4.1.12",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@playwright/test": "1.54.1",
    "@testing-library/react": "16.3.0",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.5.2",
    "daisyui": "^5.0.53",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.4",
    "jsdom": "26.1.0",
    "playwright": "1.54.1",
    "playwright-core": "1.54.1",
    "prettier": "^3.4.2",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
